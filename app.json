{
  "expo": {
    "name": "SmartKingston-QHacks",
    "slug": "smartkingston-qhacks",
    "version": "1.0.0",
    "orientation": "portrait",
    "entryPoint": "./index.js",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/react-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.7shep.smartkingston-qhacks"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow SmartKingston-QHacks to access your camera",
          "microphonePermission": "Allow SmartKingston-QHacks to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "13faff05-54a4-49d4-bfb9-e6750d50fcb7"
      },
      "router": {
        "origin": false
      }
    },
    "newArchEnabled": true
  }
}
